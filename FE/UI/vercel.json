{
  "version": 2,
  "framework": "angular",
  "outputDirectory": "dist/browser",
  "buildCommand": "chmod +x vercel-build.sh && ./vercel-build.sh",
  "installCommand": "npm install --legacy-peer-deps",
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
