{
  "version": 2,
  "framework": "angular",
  "outputDirectory": "dist/browser",
  "routes": [
    {
      "src": "/(assets|images|icons|fonts)/(.*)",
      "dest": "/$1/$2"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
